#include <stdio.h>
/**
 *main -Entry Point
 *
 *Return: Always succes 0
 */
main(void)
{
	int iahr, celsius;
	int lower, upper, step;

	lower = 0;
	upper = 300;
	step = 20;

	fahr = lower;
	fahr1 = fahr + step;
	while (fahr <= upper)
{
		celsius = 5 * (fahr - 32) / 9;
		printf("%d\t%d\n", fahr, celsius);
		fahr1 = fahr + step;
	}
}
